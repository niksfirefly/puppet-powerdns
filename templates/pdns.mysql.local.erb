# MySQL Configuration
#
# Managed by Puppet
#
# Launch gmysql backend
launch=gmysql

# gmysql parameters
gmysql-host=<%= @host %>
gmysql-dbname=<%= @dbname %>
gmysql-user=<%= @user %>
gmysql-password=<%= @password %>
<% unless @port.nil? %>
gmysql-port=<%= @port %>
<% end %>
<% unless @dnssec.nil? %>
gmysql-dnssec=<%= @dnssec %>
<% end %>
<% if @osfamily == "Redhat" %>       
<%= @cfg_include_name %>=<%= @cfg_include_path %>
<% end %>

